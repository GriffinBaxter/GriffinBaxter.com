generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id        Int      @id @default(autoincrement())
  type      String
  title     String
  subtitle  String
  published DateTime @default(dbgenerated("'2022-11-16'::date")) @db.Timestamptz(6)
  content   Json     @db.Json
  languages Json?    @db.Json
  slug      String
}
